@model WebService.Models.ConsultaCep

@{
    ViewData["Title"] = "ConsultaCep";
}

<h2>ConsultaCep</h2>

<h4>ConsultaCep</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="ConsultaCep">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Cep" class="control-label"></label>
                <input asp-for="Cep" class="form-control" />
                <span asp-validation-for="Cep" class="text-danger"></span>
            </div>            
            <div class="form-group">
                <input type="submit" value="Consultar e Cadastrar" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a><br><br>
  
</div>
